{
  "name": "canvas-test",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run install-fonts",
    "pdf": "FILETYPE=pdf node src/index.js",
    "png": "FILETYPE=png node src/index.js",
